{
  "name": "zustand",
  "description": "在原生微信小程序中, 使用 zustand 进行状态管理.",
  "private": true,
  "type": "module",
  "version": "5.0.8",
  "main": "./miniprogram/index.js",
  "types": "./miniprogram/index.d.ts",
  "miniprogram": "miniprogram",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./miniprogram/index.d.ts",
        "default": "./miniprogram/index.js"
      },
      "default": {
        "types": "./miniprogram/index.d.ts",
        "default": "./miniprogram/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./miniprogram/*.d.ts",
        "default": "./miniprogram/*.js"
      },
      "default": {
        "types": "./miniprogram/*.d.ts",
        "default": "./miniprogram/*.js"
      }
    }
  },
  "files": [
    "**"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "vite build"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halo951/zustand-mp.git"
  },
  "keywords": [
    "store",
    "state management",
    "zustand in miniprogram"
  ],
  "author": "Paul Henschel",
  "contributors": [
    "Jeremy Holcomb (https://github.com/JeremyRH)",
    "Daishi Kato (https://github.com/dai-shi)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmndrs/zustand/issues"
  },
  "homepage": "https://github.com/pmndrs/zustand",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "immer": "^10.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.9",
    "jsdom": "^26.1.0",
    "json": "^11.0.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.3",
    "rollup-plugin-esbuild": "^6.2.1",
    "shelljs": "^0.10.0",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  }
}